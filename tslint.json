{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules"
  ],
  "jsRules": {},
  "rules": {
    "arrow-parens": [true, "ban-single-arg-parens"],
    "no-namespace": false,
    "max-classes-per-file": false,
    "quotemark": [true, "single"],
    "semicolon": [true, "always"],
    "curly": [true, "ignore-same-line"]
  },
  "rulesDirectory": []
}
